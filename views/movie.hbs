<img class="card-img-top" src="{{image}}" alt="Card image cap">
<div class="card-body">
    <h2>{{title}}</h2>
    <h3>{{director}}</h3>
    <h4>Starts</h4>

    <ul>
        {{#each Starts}}
        <li>
            {{this}}
        </li>
        {{/each}}
    </ul>
    <p>{{description}}</p>

    {{#each showtimes}}
    {{this}} ||
    {{/each}}




</div>